import java.util.*;
class Solution {
    public long subarrayXor(int arr[], int k) {
        // code here
       Map<Integer,Integer> map = new HashMap<>();
       int xor = 0;
       int count = 0;
        map.put(0,1);
        for(int x:arr){
            xor = xor^x;
            
            int required = xor^ k;
            
            if(map.containsKey(required)){
                count=count+map.get(required);
            }
            map.put(xor,map.getOrDefault(xor,0)+1);
        }
         return count;
