class Solution {
    public int maxProfit(int[] prices) {
        int maxPro = 0;
        int small = Integer.MAX_VALUE;
        for(int i=0;i<prices.length;i++){
            small = Math.min(small,prices[i]);
            if(prices[i]>small){
                int max = prices[i]-small;
                maxPro = Math.max(maxPro,max);
            }

        }
        return maxPro;
    }
}
